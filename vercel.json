{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "functions": {
        "backend/server.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/server.ts"
        }
    ]
}
